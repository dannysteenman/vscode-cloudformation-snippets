{
	"CloudFormation-template": {
		"prefix": "cfn",
		"body": [
			"AWSTemplateFormatVersion: 2010-09-09",
			"Description: |",
			"\t${1}",
			"Parameters:",
			"\t",
			"Metadata:",
			"\t",
			"Mappings:",
			"\t",
			"Conditions:",
			"\t",
			"Resources:",
			"\t",
			"Transform:",
			"\t",
			"Outputs:"
		],
		"description": "The full YAML-formatted template that contains all template sections",
		"scope": "yaml"
	},
	"CloudFormation-template-lite": {
		"prefix": "cfn-lite",
		"body": [
			"AWSTemplateFormatVersion: 2010-09-09",
			"Description: |",
			"\t${1}",
			"Parameters:",
			"\t",
			"Resources:",
			"\t",
			"Outputs:"
		],
		"description": "The minimalistic YAML-formatted template that contains the necessities to get started"
	},
	"Parameter": {
		"prefix": "parameter",
		"body": [
			"${1:ParameterName}:",
			"  Description: ${2}",
			"  Type: ${3|String,Number,CommaDelimitedList|}",
			"  Default: ${4}"
		],
		"description": "",
		"scope": "yaml"
	},
	"Output": {
		"prefix": "output",
		"body": [
			"${1:LogicalID}:",
			"  Description: ${2}",
			"  Value:",
			"  Export: ",
			"    Name:"
		],
		"description": "",
		"scope": "yaml"
	},
	"Tag": {
		"prefix": "tag",
		"body": [
			"Key: ${1:keyname}",
			"Value: ${2:value}"
		]
	},
	"Map": {
		"prefix": "map",
		"body": [
			"${1:mapName}:",
			"  ${2:mapping01}:",
			"    ${3:key01}:",
			"      Name: ${4:value01}",
			"  ${5:mapping02}:",
			"    ${6:key02}:",
			"      Name: ${7:value02}"
		],
		"description": "",
		"scope": "yaml"
	},
	"security-group-egress-cidr": {
		"prefix": "security-group-egress-cidr",
		"body": [
			"IpProtocol: ${1|tcp,udp,ip|}",
			"FromPort: ${2}",
			"ToPort: ${3}",
			"CidrIp: ${4}"
		],
		"description": "",
		"scope": "yaml"
	},
	"security-group-ingress-cidr": {
		"prefix": "security-group-ingress-cidr",
		"body": [
			"IpProtocol: ${1|tcp,udp,ip|}",
			"FromPort: ${2}",
			"ToPort: ${3}",
			"CidrIp: ${4}"
		],
		"description": "",
		"scope": "yaml"
	}
}
